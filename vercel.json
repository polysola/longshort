{
  "version": 2,
  "builds": [
    {
      "src": "api/cron.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/cron",
      "dest": "/api/cron"
    },
    {
      "src": "/",
      "dest": "/api/index"
    }
  ]
}
